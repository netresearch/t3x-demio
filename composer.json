{
    "name": "netresearch/demio",
    "type": "typo3-cms-extension",
    "description": "This extension provides a plugin for the TYPO3 CMS to integrate Demio webinars into your TYPO3 website.",
    "authors": [
        {
            "name": "André Lademann",
            "role": "Developer"
        },
        {
            "name": "Philipp Madl",
            "role": "Developer"
        },
		{
			"name": "Andreas Müller",
			"role": "Developer"
		}
    ],
    "license": "GPL-2.0-or-later",
    "require": {
		"php": "^7.4.0 || ^8",
        "typo3/cms-core": "^10.4 || ^11.5 || ^12.0"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0 || ^9.0.0"
    },
    "autoload": {
        "psr-4": {
            "Netresearch\\Demio\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netresearch\\Demio\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/demio": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "demio"
        },
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
